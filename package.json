{
  "name": "clinic-schedule",
  "version": "1.0.0",
  "description": "Clinical schedule management",
  "main": "index.js",
  "author": "Yan Almeida",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/index.js",
    "start:dev": "dotenv -e .env -- ts-node-dev --transpile-only src/main.ts",
    "test": "yarn lint && yarn style:check && yarn test:unit && yarn test:functional",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "test:functional": "dotenv -e .env -- jest --projects ./test --runInBand",
    "test:unit": "dotenv -e .env -- jest",
    "style:check": "prettier --check src/**/*.ts test/**/*.ts",
    "style:fix": "prettier --write src/**/*.ts test/**/*.ts"
  },
  "dependencies": {
    "@types/module-alias": "^2.0.1",
    "body-parser": "^1.20.0",
    "colors": "^1.4.0",
    "config": "^3.3.7",
    "cors": "^2.8.5",
    "date-fns": "^2.29.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "express-pino-logger": "^7.0.0",
    "express-winston": "^4.2.0",
    "http-status-codes": "^2.2.0",
    "module-alias": "^2.2.2",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/config": "^3.3.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-pino-logger": "^4.0.3",
    "@types/jest": "^28.1.6",
    "@types/node": "18.7.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "dotenv-cli": "6",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.3",
    "pino-pretty": "^9.0.0",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "_moduleAliases": {
    "@app": "src/@app/*",
    "@core": "src/@core/*",
    "@infra": "src/@infra/*",
    "@utils": "src/utils/*",
    "@test": "test"
  }
}
